{
  "name": "blitz-oauth",
  "version": "1.0.0",
  "scripts": {
    "dev": "blitz dev",
    "start": "blitz start",
    "studio": "blitz prisma studio",
    "build": "blitz build",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "test": "jest",
    "test:watch": "jest --watch",
    "prisma:generate": "blitz prisma generate",
    "db:migrate": "blitz prisma migrate dev --preview-feature",
    "db:deploy": "blitz prisma migrate deploy --preview-feature",
    "db:reset": "blitz prisma migrate reset --preview-feature"
  },
  "browserslist": [
    "defaults"
  ],
  "prisma": {
    "schema": "db/schema.prisma"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "tsc && npm run lint && npm run test"
    }
  },
  "lint-staged": {
    "*.{js,ts,tsx}": [
      "eslint --fix"
    ]
  },
  "dependencies": {
    "prisma": "~2.17",
    "@prisma/client": "~2.17",
    "blitz": "0.30.0",
    "eslint-config-cratebind": "6.0.3",
    "formik": "2.2.6",
    "minerva-ui": "7.1.6",
    "passport-google-oauth20": "2.0.0",
    "react": "0.0.0-experimental-3310209d0",
    "react-dom": "0.0.0-experimental-3310209d0",
    "react-error-boundary": "3.1.0",
    "typescript": "4.1.4",
    "zod": "1.11.11"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "5.11.9",
    "@testing-library/react": "11.2.5",
    "@testing-library/react-hooks": "3.7.0",
    "@types/jest": "26.0.20",
    "@types/react": "17.0.1",
    "@typescript-eslint/eslint-plugin": "4.15.0",
    "@typescript-eslint/parser": "4.15.0",
    "babel-eslint": "10.1.0",
    "eslint": "7.19.0",
    "eslint-config-react-app": "6.0.0",
    "eslint-plugin-flowtype": "5.2.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-react": "7.22.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "4.3.8",
    "jest": "26.6.3",
    "jest-environment-jsdom-fourteen": "1.0.1",
    "jest-watch-typeahead": "0.6.1",
    "lint-staged": "10.5.4",
    "react-test-renderer": "17.0.1",
    "ts-jest": "26.5.1"
  },
  "private": true
}
